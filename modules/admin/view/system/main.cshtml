    <script>
        // If you want to draw your charts with Theme colors you must run initiating charts after that current skin is loaded
        $(window).bind("load", function () {

            /*Sets Themed Colors Based on Themes*/
            themeprimary = getThemeColorFromCss('themeprimary');
            themesecondary = getThemeColorFromCss('themesecondary');
            themethirdcolor = getThemeColorFromCss('themethirdcolor');
            themefourthcolor = getThemeColorFromCss('themefourthcolor');
            themefifthcolor = getThemeColorFromCss('themefifthcolor');

            InitiateAreaByHours.init("user/GetLoginNum",
                                "area-chart-login",
                                "time",
                                ["num"],
                                ["次数"]);
        });
        var admin_list_vm = new Vue({
            el: '#app',
            data: 
            {
                datas:[],
                users:[],
                cur: 0,
                all: 0,
                total: 0,
                url:window.location.href
            },
            components:
            {
                'vue-nav': Vnav
            },
            created()
            {
                this.onPage(1)
            },
            methods:
            {
                convertTime:function(time)
                {
                    return moment(time).format('YYYY-M-D H:mm:ss')
                },
                onPage:function(page)
                {
                    this.updateDatas(page-1)
                },
                updateDatas:function(page)
                {
                    let self = this
                    $.ajax({
                        type: "POST",
                        url: "/system/AdminLoginList",
                        dataType: 'json',
                        data:{
                            page:page,
                            page_num:20
                        },
                        xhrFields: {
                            withCredentials: true
                        },
                        crossDomain: true,
                        success:function(response)
                        {
                            if(response.err)
                            {
                                layer.msg(response.err, {icon: 5, time: 2000})
                                return
                            }
                            self.cur=response.page
                            self.all=response.total_page
                            self.total=response.total_num
                            self.datas=response.datas||[]
                        },
                        error:function(){
                    
                        },
                    })
                }
            }
        });
    </script>
